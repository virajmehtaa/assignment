#include <stdio.h>

int main() {

    int arr1[5];

    printf("Enter 5 integers for 1D array:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr1[i]);
    }

    printf("1D Array Elements: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr1[i]);
    }
    printf("\n\n");

    int arr2[3][3];
    int sum = 0;

    printf("Enter 9 integers for 3x3 matrix:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            scanf("%d", &arr2[i][j]);
        }
    }

    printf("3x3 Matrix:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", arr2[i][j]);
            sum += arr2[i][j];
        }
        printf("\n");
    }

    printf("\nSum of all elements in 3x3 matrix: %d\n", sum);

    return 0;
}
