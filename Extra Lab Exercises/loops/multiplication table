#include<stdio.h>

int main()
{
    int num, N, i;
    
    printf("Enter a number: ");
    scanf("%d", &num);
    
    printf("Enter the range (e.g., up to N): ");
    scanf("%d", &N);
    
    printf("Multiplication Table of %d up to %d:\n", num, N);
    
    for(i=1; i<=N; i++)
    {
        printf("%d x %d = %d\n", num, i, num*i);
    }
    
    return 0;
}
